            <tbody><tr><td class='acq-lit-table-spacer' colspan='0'></td></tr></tbody>
            <tbody style='font-weight:bold;'>
                <tr>
                    <td style='margin-top:30px;'>Owning Branch</td>
                    <td>Shelving Location</td>
                    <td>Collection Code</td>
                    <td>Fund</td>
                    <td>Circ Modifier</td>
                    <td>Callnumber</td>
                    <td>Barcode</td>
                    <td>Notes</td>
                    <td colspan='0'></td>
                </tr>
            </tbody>
